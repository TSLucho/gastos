<div class="text-center">
<h2>Nuevo Usuario</h2>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>
  <div class="field">
    <%= f.text_field :username, autocomplete: "username", placeholder: "Nombre de Usuario" %>
  </div>
  <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Correo" %>
  </div>
  <div class="field">
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> Caracteres Minimos)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "password", placeholder: "Contraseña" %>
  </div>
  <div class="field">
    <%= f.password_field :password_confirmation, autocomplete: "password-confirmation", placeholder: "Repetir Contraseña" %>
  </div>
  <div class="actions">
    <%= f.submit "Crear Usuario", :class => 'btn btn-primary boton-login' %>
  </div>
<% end %>
</div>